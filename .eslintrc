{
  "rules": {
    "eqeqeq": "off",
    "no-useless-escape": "off",
    "prefer-destructuring": "error",
    "prefer-template": "error",
    "no-console": "off"
  },
  "env": {
    "node": true,
    "es6": true
  },
  "extends": "eslint:recommended",
  "parserOptions": {
    "ecmaVersion": 2017
  }
}
